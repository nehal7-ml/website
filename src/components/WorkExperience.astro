---
interface Props {
	title: string;
	description: string;
	startDate: string;
	endDate: string;
}

const { title, description, startDate, endDate } = Astro.props;
---

<li class="grid mb-5">
  <div class="flex gap-5 justify-between mb-3">
   <h3 class="text-xl font-bold">
     {title}
   </h3>
   <p class="text-sm"> 
    {startDate} - {endDate}
   </p>
  </div> 
  <p>
		{description}
	</p>
</li>
